{
  "version": 2,
  "public": "dist/public",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/public/index.html"
    }
  ],
  "builds": [
    {
      "src": "server/index.ts",
      "use": "@vercel/node"
    }
  ]
}
