{
  "version": 2,
  "builds": [
    {
      "src": "client/index.html",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "server/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/client/index.html"
    }
  ],
  "env": {
    "DATABASE_URL": "postgres://neondb_owner:npg_Jkp5aYZAr0vW@ep-plain-moon-a4o4stqr-pooler.us-east-1.aws.neon.tech/neondb?sslmode=require"
  },
  "buildCommand": "npm run build"
}
